{
	"schemaVersion": 1,
	"id": "otel",
	"version": "${version}",
	"name": "OpenTelemetry Log Instrumentation",
	"description": "This server mod installs the OpenTelemetry Log4j2 Appender, netting you remote logging capabilities",
	"authors": [
		"Eleanor Young <developer@nausicaea.net>"
	],
	"contact": {
		"homepage": "https://gitlab.com/nausicaea/otel",
		"sources": "https://gitlab.com/nausicaea/otel"
	},
	"license": "Apache-2.0",
	"icon": "assets/otel/icon.png",
	"environment": "server",
	"entrypoints": {
		"preLaunch": [
          "net.nausicaea.otel.Otel"
		]
	},
	"mixins": [
		"otel.mixins.json"
	],
	"depends": {
		"fabricloader": "~${fabric_loader_version}",
		"minecraft": "~${minecraft_version}",
		"java": ">=21"
	}
}
